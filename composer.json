{
    "name": "brain-diminished/schema-version-control",
    "license": "MIT",
    "authors": [
        {
            "name": "Dorian Savina",
            "email": "dorian.savina@gmail.com"
        }
    ],
    "require": {
        "doctrine/dbal": "^2.8",
        "symfony/yaml": "^4.1 | ^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "satooshi/php-coveralls": "^1.0.1",
        "thecodingmachine/dbal-fluid-schema-builder": "^1.3.0"
    },
    "autoload": {
        "psr-4": {
            "BrainDiminished\\SchemaVersionControl\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "BrainDiminished\\SchemaVersionControl\\Test\\" : "tests/"
        }
    },
    "archive": {
        "exclude": ["tests", "*phpunit.xml"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
